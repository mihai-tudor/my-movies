{
  "extends": "standard",
  "plugins": [
    "import",
    "html"
  ],
  "parserOptions": {
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "node": true
  },
  "parser": "babel-eslint",
  "rules": {
    "no-console": [0],
    "semi": [0],
    "no-use-before-define": [0],
    "no-confusing-arrow": [0],
    "no-underscore-dangle": [0],
    "function-paren-newline": [0],
    "import/no-extraneous-dependencies": [0],
    "arrow-parens": [0],
    "generator-star-spacing": [0],
    "comma-dangle": ["error", {
      "functions": "ignore"
    }]
  }
}
